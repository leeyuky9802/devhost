{
  "name": "devhost",
  "version": "1.0.10",
  "type": "module",
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "dev": "node src/index.ts",
    "star": "node build/dist/index.cjs",
    "build": "rm -rf build && esbuild src/index.ts --bundle --minify --platform=node --target=node20 --outfile=build/dist/index.cjs --banner:js='#!/usr/bin/env node' && cp -r src/bin build/bin && cp LICENSE build/LICENSE && node scripts/generate-build-package-json.js",
    "lint": "eslint",
    "test": "node --use-system-ca tests/www.example.com.test.ts"
  },
  "imports": {
    "#*": "./src/*.ts"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}
